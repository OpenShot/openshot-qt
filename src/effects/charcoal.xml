<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Charcoal</title>
	<description translatable="True">Filter the image to look like it is drawn with charcoal</description>
	<icon>charcoal.png</icon>
	<category>Video</category>
	<service>charcoal</service>

	<param name="x_scatter" type="spinner" title="X Scatter" description="">
		<min>0.0</min>
		<max>2.0</max>
		<default>1.0</default>
	</param>

	<param name="y_scatter" type="spinner" title="Y Scatter" description="">
		<min>0.0</min>
		<max>2.0</max>
		<default>1.0</default>
	</param>

	<param name="scale" type="spinner" title="Scale" description="">
		<min>0.0</min>
		<max>2.0</max>
		<default>1.0</default>
	</param>

	<param name="mix" type="dropdown" title="Mix" description="">
		<values>
			<value name="Yes" num="1.0"/>
			<value name="No" num="0.0"/> 
		</values>
		<default>1.0</default>
	</param>
	
</effect>
