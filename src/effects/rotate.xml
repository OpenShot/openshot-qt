<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Rotate</title>
	<description translatable="True">Rotate an image statically or animated</description>
	<icon>rotate.png</icon>
	<category>Video</category>
	<service>affine</service>
	
	<param name="transition.rotate_x" type="spinner" title="Rotate X" description="">
		<min>-30</min>
		<max>30</max>
		<step>1.0</step>
		<default>1.5</default>
	</param>
	<param name="transition.rotate_y" type="spinner" title="Rotate Y" description="">
		<min>-30</min>
		<max>30</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>
	<param name="transition.rotate_z" type="spinner" title="Rotate Z" description="">
		<min>-30</min>
		<max>30</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>
	
	<param name="transition.fix_rotate_x" type="spinner" title="Fixed Rotate X" description="">
		<min>-1800</min>
		<max>1800</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>
	<param name="transition.fix_rotate_y" type="spinner" title="Fixed Rotate Y" description="">
		<min>-1800</min>
		<max>1800</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>
	<param name="transition.fix_rotate_z" type="spinner" title="Fixed Rotate Z" description="">
		<min>-1800</min>
		<max>1800</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>
	
	
	<param name="transition.ox" type="spinner" title="X Offset" description="">
		<min>-1800</min>
		<max>1800</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>
	<param name="transition.oy" type="spinner" title="Y Offset" description="">
		<min>-1800</min>
		<max>1800</max>
		<step>1.0</step>
		<default>0.0</default>
	</param>

	
	

</effect>