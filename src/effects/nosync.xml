<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Nosync</title>
	<description translatable="True">Adjust the image up and down, with the off-screen portion appearing on the other side</description>
	<icon>nosync.png</icon>
	<category>Video</category>
	<service>frei0r.nosync0r</service>
	
	<param name="HSync" type="spinner" title="Horizontal Sync" description="">
		<min>0.0</min>
		<max>1.0</max>
		<step>0.01</step>
		<default>0.0</default>
	</param>

</effect>

