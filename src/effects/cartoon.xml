<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Cartoon</title>
	<description translatable="True">Cartoonify the image, using a form of edge detection</description>
	<icon>cartoon.png</icon>
	<category>Video</category>
	<service>frei0r.cartoon</service>
	
	<param name="triplevel" type="spinner" title="Trip Level" description="">
		<min>0.0</min>
		<max>2000.0</max>
		<step>1.0</step>
		<default>300.0</default>
	</param>

	<param name="diffspace" type="spinner" title="Diff Space" description="">
		<min>0.0</min>
		<max>256.0</max>
		<step>1.0</step>
		<default>1.0</default>
	</param>


</effect>

