<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Magic Wand</title>
	<description translatable="True">Magic Wand</description>
	<icon>magic_wand.png</icon>
	<category>Video</category>
	<service>magic_wand.blend</service>
	
	<param name="file_name" type="text" title="File Name" description="">
		<default>TitleFileName</default>
	</param>
	
	<param name="particles_count" type="spinner" title="Particles: Amount" description="">
		<min>0.0</min>
		<max>100000.0</max>
		<default>5000.0</default>
	</param>
	
	<param name="start_x" type="spinner" title="Start: X" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>
	
	<param name="start_y" type="spinner" title="Start: Y" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>
	
	<param name="start_z" type="spinner" title="Start: Z" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>3.0</default>
	</param>
	
	<param name="end_x" type="spinner" title="End: X" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>
	
	<param name="end_y" type="spinner" title="End: Y" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>
	
	<param name="end_z" type="spinner" title="End: Z" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>3.0</default>
	</param>

	<param name="diffuse_color" type="color" title="Diffuse Color" description="">
		<default>#FFCF00</default>
	</param>
	
	<param name="specular_color" type="color" title="Specular Color" description="">
		<default>#ffffff</default>
	</param>
	
	<param name="mirror_color" type="color" title="Mirror Color" description="">
		<default>#FF00BA</default>
	</param>
	
	<param name="specular_intensity" type="spinner" title="Specular Intensity" description="">
		<min>0.0</min>
		<max>1.0</max>
		<default>0.5</default>
	</param>
	
	<param name="alpha" type="spinner" title="Alpha" description="">
		<min>0.0</min>
		<max>1.0</max>
		<default>0.3</default>
	</param>
	
	
	<param name="halo_size" type="spinner" title="Halo: Size" description="">
		<min>0.0</min>
		<max>1.0</max>
		<default>0.2</default>
	</param>
	
	<param name="halo_hardness" type="spinner" title="Halo: Hardness" description="">
		<min>0.0</min>
		<max>100.0</max>
		<default>68.0</default>
	</param>
	
	<param name="halo_use_lines" type="dropdown" title="Halo: Use Lines" description="">
		<values>
			<value name="Yes" num="Yes"/>
			<value name="No" num="No"/>
		</values>
		<default>Yes</default>
	</param>
	<param name="halo_line_count" type="spinner" title="Halo: Line Count" description="">
		<min>0.0</min>
		<max>100.0</max>
		<default>12.0</default>
	</param>
	
	<param name="halo_use_ring" type="dropdown" title="Halo: Use Rings" description="">
		<values>
			<value name="Yes" num="Yes"/>
			<value name="No" num="No"/>
		</values>
		<default>Yes</default>
	</param>
	<param name="halo_ring_count" type="spinner" title="Halo: Ring Count" description="">
		<min>0.0</min>
		<max>100.0</max>
		<default>4.0</default>
	</param>
	
	<param name="halo_use_star" type="dropdown" title="Halo: Use Stars" description="">
		<values>
			<value name="Yes" num="Yes"/>
			<value name="No" num="No"/>
		</values>
		<default>Yes</default>
	</param>
	<param name="halo_star_tip_count" type="spinner" title="Halo: Star Count" description="">
		<min>0.0</min>
		<max>100.0</max>
		<default>4.0</default>
	</param>
	
	<param name="halo_use_flare_mode" type="dropdown" title="Halo: Use Flare" description="">
		<values>
			<value name="Yes" num="Yes"/>
			<value name="No" num="No"/>
		</values>
		<default>No</default>
	</param>

	<param name="particles_lifetime" type="spinner" title="Particles: Lifetime" description="">
		<min>0.0</min>
		<max>255.0</max>
		<default>50.0</default>
	</param>	
	
	<param name="particles_gravity" type="spinner" title="Particles: Gravity" description="">
		<min>0.0</min>
		<max>1.0</max>
		<default>1.0</default>
	</param>
	
	<param name="velocity_x" type="spinner" title="Velocity: X" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>
	
	<param name="velocity_y" type="spinner" title="Velocity: Y" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>
	
	<param name="velocity_z" type="spinner" title="Velocity: Z" description="">
		<min>-10.0</min>
		<max>10.0</max>
		<default>0.0</default>
	</param>

	
	<param name="start_frame" type="spinner" title="Start Frame" description="">
		<min>1</min>
		<max>1</max>
		<default>1</default>
	</param>
	
	<param name="end_frame" type="spinner" title="End Frame" description="">
		<min>250</min>
		<max>250</max>
		<default>250</default>
	</param>

	<!-- SPEED -->
	<param name="animation_speed" type="dropdown" title="Animation Length" description="">
		<values>
			<value name="Default" num="1"/>
			<value name="2X" num="2"/>
			<value name="3X" num="3"/>
			<value name="4X" num="4"/>
			<value name="5X" num="5"/>
		</values>
		<default>1</default>
	</param>
</effect>
